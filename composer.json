{
    "name": "gintonicweb/images",
    "description": "Images plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "3.1.*",
        "josegonzalez/cakephp-upload": "3.x-dev",
        "league/glide": "dev-master",
        "admad/cakephp-glide": "dev-master",
        "gintonicweb/requirejs": "~0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "cakephp/cakephp-codesniffer": "2.*"
    },
    "autoload": {
        "psr-4": {
            "Images\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Images\\Test\\": "tests"
        }
    }
}
